{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.json",
    "src/**/*.vue",
    "auto-imports.d.ts",
    "components.d.ts",
    "src/components/echarts/data/china.json"
  ],
  "exclude": ["src/**/__tests__/*"],
  // "exclude": ["node_moudles"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noImplicitAny": false,
    "moduleResolution": "node"
  }
}
